        <mxCell id="Z9WvmetvKEbyeU4UL3iK-27" value="&lt;b&gt;&lt;i&gt;Identify Endpoints And Keys&lt;/i&gt;&lt;/b&gt;" style="whiteSpace=wrap;html=1;rounded=1;strokeColor=#d79b00;fillColor=#ffe6cc;" parent="1" vertex="1">
