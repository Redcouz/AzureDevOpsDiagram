        <mxCell id="G9OUEWUEo2JU_yxyGa9J-14" value="&lt;b&gt;&lt;i&gt;Text and Speech Translation&lt;/i&gt;&lt;/b&gt;" style="whiteSpace=wrap;html=1;rounded=1;strokeColor=#9673a6;fillColor=#e1d5e7;" parent="1" vertex="1">
