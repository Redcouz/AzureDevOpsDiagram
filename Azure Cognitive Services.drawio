        <mxCell id="G9OUEWUEo2JU_yxyGa9J-60" value="&lt;b&gt;&lt;i&gt;Standard Voice&lt;/i&gt;&lt;/b&gt;" style="whiteSpace=wrap;html=1;rounded=1;strokeColor=#B09500;fillColor=#DAE39D;" parent="1" vertex="1">
