        <mxCell id="t2yQqbgyM3sy9qxiI3k3-1" value="Azure Resources" style="whiteSpace=wrap;html=1;rounded=1;strokeColor=#6c8ebf;fillColor=#dae8fc;" parent="1" vertex="1">
