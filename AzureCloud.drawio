        <mxCell id="l9tMWQ5-sVlqTfUE8LL0-51" value="&lt;font style=&quot;font-size: 14px&quot;&gt;&lt;i&gt;Capital Expenditure(CapEx)&lt;/i&gt;&lt;/font&gt;" style="ellipse;whiteSpace=wrap;html=1;rounded=1;strokeColor=#6c8ebf;strokeWidth=2;fillColor=#dae8fc;hachureGap=4;" parent="1" vertex="1">
