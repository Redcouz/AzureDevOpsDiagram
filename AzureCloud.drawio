        <mxCell id="l9tMWQ5-sVlqTfUE8LL0-11" value="&lt;i&gt;&lt;font style=&quot;font-size: 16px&quot;&gt;Hybrid Cloud&lt;/font&gt;&lt;/i&gt;" style="whiteSpace=wrap;html=1;rounded=1;strokeColor=#6c8ebf;strokeWidth=2;fillColor=#dae8fc;hachureGap=4;" parent="1" vertex="1">
